<section class="skill-set animate">
    <div class="container">
        <h2>
            Combining
            <span class="underline--magical"><a
                    href="https://artsandculture.google.com/"
                    target="_blank"
                >Art</a>
            </span>
            &
            <span class="underline--magical"><a
                    href="https://developer.mozilla.org/en-US/docs/Learn"
                    target="_blank"
                >
                    Code</a></span>
        </h2>
        <ul>
            {% for skill in skillSets %}
            <li>
                <div class="art-container">
                    <div class="art">
                        <img
                            src="{{skill.img}}"
                            alt=""
                        />
                    </div>
                </div>
                <p>{{skill.skill}}</p>
            </li>
            {% endfor %}
        </ul>
    </div>
    <div class="triangle"></div>
</section>
